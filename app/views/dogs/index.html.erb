<h1>DOGS INDEX PAGE</h1>

<% @pets.each do |dog| %>
	<p>
	NAME: <%= dog.name %><br>
	AGE: <%= dog.age %><br>
	ID: <%= dog.id %><br>
	SEX: <%= dog.sex %><br>
	MIX: <%= dog.mix %><br>
	SHELTERID: <%= dog.shelter_id %><br>
	EMAIL: <%= dog.contact %><br>
	STATUS: <%= dog.status %>
	<br>
	SIZE: <%= dog.size %>
	<br>
	<span> PHOTO: <%#= dog.photo %> </span>
	<br>
	<b>TEST: <%#= dog.media[1] %>
	<%= dog.contact %>
	</b>
	<br>
	LAST UDPATED:
	<%= dog.last_update %>
	<br>
	<span><a>BREED:::FIRST BREED IN BREED ARRAY</a><%= dog.breeds[0] %></span>
	<%#= dog.media.photos.photo(1) %> <br>
	</p>
<% end %>

